config_version=5

[application]

config/name="Sands of Duat"
config/description="ARPG roguelike isométrico 3D inspirado em Hades, com mitologia egípcia. Khenti, um príncipe assassinado, luta para escapar do Duat e retornar ao mundo dos vivos."
run/main_scene="res://scenes/TestScene.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/core/GameManager.gd"
AudioManager="*res://scripts/core/AudioManager.gd"
SceneManager="*res://scripts/core/SceneManager.gd"

[debug]

gdscript/warnings/enable=true
gdscript/warnings/treat_warnings_as_errors=false
gdscript/warnings/exclude_addons=true

[display]

